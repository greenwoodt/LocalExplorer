<h2> Fill your Profile information here</h2>

<div class="banner-createprofile">
  <%= simple_form_for(@profile) do |f| %>
    <div class="container-inputform">
      <div class="form-inputs">
        <%= f.input :username %>
        <%= f.input :first_name %>
        <%= f.input :last_name %>
        <%= f.input :description %>
        <%= f.input :front_driver_license, as: :file %>
        <%= f.input :back_driver_license, as: :file %>
        <%= f.input :profile_photo, as: :file %>
      </div>
      <div class="button">
        <%= f.button :submit %>
      </div>
    </div>
  <% end %>
</div>
