<div class="body-overall-container">
    <div class="heading-my-trips">
        <h2>My Trips</h2>
    </div>
    <div class="container-trip-index-cards-mytrips">
        <% if current_user.profile != nil %>
            <% current_user.trips.each do |trip|  %>
                <%= render "trip_index_card", trip: trip %>
            <%end %>
        <%end%>
    </div>
</div>